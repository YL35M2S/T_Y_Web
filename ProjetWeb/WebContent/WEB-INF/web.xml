<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>ProjetWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>connexion</servlet-name>
    <servlet-class>proj.servlets.connexion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>connexion</servlet-name>
    <url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>accueillir</servlet-name>
    <servlet-class>proj.servlets.accueillir</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>accueillir</servlet-name>
    <url-pattern>/accueillir</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deconnexion</servlet-name>
    <servlet-class>proj.servlets.deconnexion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deconnexion</servlet-name>
    <url-pattern>/deconnexion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>bonjour</servlet-name>
    <servlet-class>proj.servlets.bonjour</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bonjour</servlet-name>
    <url-pattern>/bonjour</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>fiche</servlet-name>
    <servlet-class>proj.servlets.Fiche</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fiche</servlet-name>
    <url-pattern>/fiche</url-pattern>
    </servlet-mapping>
  <servlet>
    <servlet-name>video</servlet-name>
    <servlet-class>proj.servlets.video</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>video</servlet-name>
    <url-pattern>/video</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Inscription</servlet-name>
    <servlet-class>proj.servlets.Inscription</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Inscription</servlet-name>
    <url-pattern>/inscription</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Upload</servlet-name>
    <servlet-class>proj.servlets.Upload</servlet-class>
    <init-param>
      <param-name>chemin</param-name>
      <param-value>/Users/PerrinTheo/Documents/workspaceWeb/upload/</param-value>
    </init-param>
    <multipart-config>
      <location>/Users/PerrinTheo/Documents</location>
      <max-file-size>10485760</max-file-size>
      <max-request-size>52428800</max-request-size>
      <file-size-threshold>1048576</file-size-threshold>
    </multipart-config>
  </servlet>
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Download</servlet-name>
    <servlet-class>proj.servlets.Download</servlet-class>
    <init-param>
      <param-name>chemin</param-name>
      <param-value>/Users/PerrinTheo/Documents/workspaceWeb/upload/</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Download</servlet-name>
    <url-pattern>/fichiers/*</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>proj.config.InitialisationDaoFactory</listener-class>
  </listener>
</web-app>